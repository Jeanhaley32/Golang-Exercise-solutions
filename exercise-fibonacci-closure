package main

import "fmt"

// fibonacci is a function that returns
// a function that returns an int.
func fibonacci() func(i int) int {
	a := 0
	b := 1
	c := 0
	return func(i int) int{
		if i == 0 {
			return a
		}
		if i == 1 {
			return b
		}else{
			c = b + a
			a = b
			b = c
			return c
		}
	}
}

func main() {
	f := fibonacci()
	for i := 0; i < 10; i++ {
		fmt.Println(f(i))
	}
}
